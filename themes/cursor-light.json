  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": { "foreground": "#141414AD", "fontStyle": "italic" }
    },
    {
      "name": "Strings",
      "scope": ["string", "string punctuation.section.embedded source"],
      "settings": { "foreground": "#9E94D5" }
    },
    {
      "name": "String Punctuation",
      "scope": "punctuation.definition.string.begin,punctuation.definition.string.end",
      "settings": { "foreground": "#9E94D5" }
    },
    {
      "name": "Variables in Strings",
      "scope": "string variable",
      "settings": { "foreground": "#206595" }
    },
    {
      "name": "String interpolation delimiters",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded"
      ],
      "settings": { "foreground": "#141414AD" }
    },
    {
      "name": "Regular Expressions",
      "scope": ["source.regexp", "string.regexp"],
      "settings": { "foreground": "#3C7CAB" }
    },
    {
      "name": "Regex Escape Characters",
      "scope": "string.regexp constant.character.escape",
      "settings": { "foreground": "#1F8A65" }
    },
    {
      "name": "Regex Operators",
      "scope": [
        "keyword.operator.quantifier.regexp",
        "keyword.operator.negation.regexp",
        "keyword.operator.alternation.regexp",
        "keyword.operator.or.regexp",
        "keyword.control.anchor.regexp"
      ],
      "settings": { "foreground": "#A33900" }
    },
    {
      "name": "Regex Punctuation",
      "scope": [
        "punctuation.definition.group.regexp",
        "punctuation.definition.group.assertion.regexp",
        "punctuation.definition.character-class.regexp",
        "punctuation.character.set.begin.regexp",
        "punctuation.character.set.end.regexp",
        "support.other.parenthesis.regexp"
      ],
      "settings": { "foreground": "#DB704B" }
    },
    {
      "name": "Character Constants & Placeholders",
      "scope": ["constant.other.placeholder", "constant.character"],
      "settings": { "foreground": "#B3003F" }
    },
    {
      "name": "Constants & Language Variables",
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.language"
      ],
      "settings": { "foreground": "#206595" }
    },
    {
      "name": "Integers",
      "scope": "constant.numeric",
      "settings": { "foreground": "#B8448B" }
    },
    {
      "name": "Support Constants",
      "scope": "support.constant",
      "settings": { "foreground": "#206595" }
    },
    {
      "name": "Language variables",
      "scope": "variable.language",
      "settings": { "foreground": "#CF2D56" }
    },
    {
      "name": "Support Variables",
      "scope": "support.variable",
      "settings": { "foreground": "#206595" }
    },
    {
      "name": "Variables (Readwrite)",
      "scope": "variable.other.readwrite",
      "settings": { "foreground": "#206595" }
    },
    {
      "name": "Keywords",
